    "options": {
        "default": {
            "wrap": {
                "highgui": "xvfb-run -a"
            },
            "args": {
                "test": "--skip_unstable=1",
                "perf": "--perf_impl=plain --perf_min_samples=1 --perf_force_samples=1 --perf_verify_sanity --skip_unstable=1"
            },
            "exe": {
                "opencv_test_dnn_classic": "opencv_test_dnn"
            },
            "env": {
                "opencv_test_dnn_classic": {
